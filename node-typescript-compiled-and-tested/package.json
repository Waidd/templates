{
  "name": "node-typescript-compiled",
  "version": "0.0.0",
  "description": "template for node with compiled typescript project",
  "type": "module",
  "engines": {
    "node": ">=18.2.0"
  },
  "scripts": {
    "start": "node dist/start.js",
    "build": "rm -rf ./dist && npm run compile",
    "watch": "npm run compile -- --watch",
    "compile": "tsc",
    "lint": "xo",
    "test": "jest",
    "coverage": "npm run test -- --coverage"
  },
  "author": "Thomas Cholley",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.36",
    "jest": "^28.1.0",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.2",
    "xo": "^0.49.0"
  }
}
